{
"manifest_version": 3,
"name": "Auto Lead Keyword Detector",
"version": "1.0",
"description": "Automatically clicks Contact Buyer based on dynamic keywords",
"permissions": ["storage", "activeTab", "scripting"],
"host_permissions": [
"https://*.indiamart.com/*",
"https://ext-sys.onrender.com/*"
],


"content_scripts": [
{
   "matches": ["https://seller.indiamart.com/bltxn/?pref=recent"],
"js": ["content.js"],
"run_at": "document_idle"
}
]
}